    "items": [
      "./bizfly-agent.exe",
      "./etc/bizfly-agent/bizfly-agent.yaml"
    ]
  },
  "env": {
    "guid": "94faac3d-4478-431c-8497-fba55dcfb249",
    "vars": [{
      "name": "PATH",
      "value": "[INSTALLDIR]",
      "permanent": "yes",
      "system": "no",
      "action": "set",
      "part": "last"
    }]
  },
  "shortcuts": {},
  "hooks": [{
    "when": "install",
    "command": "sc.exe create BizFlyAgent binPath=\"[INSTALLDIR]bizfly-agent.exe --collector.processes\" type=share start=auto DisplayName=\"BizFly Agent\""
  }, {
    "when": "install",
    "command": "sc.exe start BizFlyAgent"
  }, {
    "when": "uninstall",
    "command": "sc.exe delete BizFlyAgent"
  }],
  "choco": {
    "description": "Use BizFly Agent to do monitor your systems",
